<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Solve It"
        android:textSize="50dp" />

    <RelativeLayout
        android:id="@+id/grup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center">
        <Button
            android:id="@+id/btn1"
            style="@style/ButtonS"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:tag="00"
            android:text="1">
        </Button>

        <Button
            android:id="@+id/btn2"
            style="@style/ButtonS"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@id/btn1"
            android:tag="01"
            android:text="2">
        </Button>
        <Button
            android:id="@+id/btn3"
            style="@style/ButtonS"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@id/btn2"
            android:tag="02"
            android:text="3">
        </Button>

        <Button
            android:id="@+id/btn4"
            style="@style/ButtonS"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@id/btn3"
            android:tag="03"
            android:text="4">
        </Button>
        <Button
            android:id="@+id/btn5"
            style="@style/ButtonS"
            android:layout_alignParentStart="true"
            android:layout_below="@id/btn1"
            android:tag="10"
            android:text="5">
        </Button>
        <Button
            android:id="@+id/btn6"
            style="@style/ButtonS"
            android:layout_below="@id/btn1"
            android:layout_toEndOf="@id/btn1"
            android:tag="11"
            android:text="6">
        </Button>
        <Button
            android:id="@+id/btn7"
            style="@style/ButtonS"
            android:layout_below="@id/btn1"
            android:layout_toEndOf="@id/btn2"
            android:tag="12"
            android:text="7">
        </Button>
        <Button
            android:id="@+id/btn8"
            style="@style/ButtonS"
            android:layout_below="@id/btn1"
            android:layout_toEndOf="@id/btn3"
            android:tag="13"
            android:text="8">
        </Button>
        <Button
            android:id="@+id/btn9"
            style="@style/ButtonS"
            android:layout_alignParentStart="true"
            android:layout_below="@id/btn5"
            android:tag="20"
            android:text="9">
        </Button>
        <Button
            android:id="@+id/btn10"
            style="@style/ButtonS"
            android:layout_below="@id/btn5"
            android:layout_toEndOf="@id/btn1"
            android:tag="21"
            android:text="10">
        </Button>
        <Button
            android:id="@+id/btn11"
            style="@style/ButtonS"
            android:layout_below="@id/btn5"
            android:layout_toEndOf="@id/btn2"
            android:tag="22"
            android:text="11">
        </Button>
        <Button
            android:id="@+id/btn12"
            style="@style/ButtonS"
            android:layout_below="@id/btn5"
            android:layout_toEndOf="@id/btn3"
            android:tag="23"
            android:text="12">
        </Button>
        <Button
            android:id="@+id/btn513"
            style="@style/ButtonS"
            android:layout_alignParentStart="true"
            android:layout_below="@id/btn9"
            android:tag="30"
            android:text="13">
        </Button>
        <Button
            android:id="@+id/btn14"
            style="@style/ButtonS"
            android:layout_below="@id/btn9"
            android:layout_toEndOf="@id/btn1"
            android:tag="31"
            android:text="14">
        </Button>
        <Button
            android:id="@+id/btn15"
            style="@style/ButtonS"
            android:layout_below="@id/btn9"
            android:layout_toEndOf="@id/btn2"
            android:tag="32"
            android:text="15">
        </Button>
        <Button
            android:id="@+id/btn16"
            style="@style/ButtonS"
            android:layout_below="@id/btn9"
            android:layout_toEndOf="@id/btn3"
            android:tag="33"
            android:background="@color/colorBebas"
            android:text="">
        </Button>
    </RelativeLayout>
</FrameLayout>